<%= image_tag "food.jpg", id: "food" %>
<%= image_tag "logo.jpeg", id: "logo" %>
<%= image_tag "hotdog.jpeg", id: "hotdog" %>

<% sizes = [1,2,3] %>
<% @messages.each do |message| %>
  <%= sender = Phone_numbers[message.from_number] || "Unknown" %>
  <h<%= sizes.sample %>><%= "#{sender}: #{message.body}" %></h<%= sizes.sample %>>
<% end %>
