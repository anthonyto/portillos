<!DOCTYPE html>
<html>
<head>
  <title>Portillos</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
  <% end %>

  <%= link_to "Send a text", new_message_path %> <%= link_to "Responses", messages_path %> <%= link_to "What is this", info_path %>
  <br>

  <%= yield %>
  
</body>
</html>
